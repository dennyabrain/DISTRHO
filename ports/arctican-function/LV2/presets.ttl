        lv2:symbol "gain" ;
        pset:value 1 ;
    ] ;
    lv2:port [
        lv2:symbol "gain_l" ;
        pset:value 1 ;
    ] ;
    lv2:port [
        lv2:symbol "gain_r" ;
        pset:value 1 ;
    ] ;
    lv2:port [
        lv2:symbol "phase_l" ;
        pset:value 0 ;
    ] ;
    lv2:port [
        lv2:symbol "phase_r" ;
        pset:value 0 ;
    ] ;
    lv2:port [
        lv2:symbol "pan_l" ;
        pset:value 0 ;
    ] ;
    lv2:port [
        lv2:symbol "pan_r" ;
        pset:value 1 ;
    ] .

